#include <stdio.h>
#include <math.h>
int sum(int a, int b);  //Function Declaration


int main()
{   int a,b,s=0;
    printf("Enter two number");
    scanf("%d%d",&a,&b);
    s=sum(a,b);             //Function Calling
    return 0;
}
int sum(int x, int y){      //Function Defination
    int sum=0;
    sum=x+y;                //---Function body
    return sum;             // We can't define one function in another function
}
